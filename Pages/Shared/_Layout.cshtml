﻿<!DOCTYPE html>
<html lang="zh-CN">
<head>
	<meta charset="utf-8"/>
	<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
	<meta name="author" content="execute233"/><meta name="renderer" content="webkit"/>
	<link rel="icon" href="~/favicon.svg"/><link rel="manifest" href="~/site.webmanifest">
	<link rel="stylesheet" href="~/css/style/dark.css"/><link rel="stylesheet" href="~/css/nav/dark.css"/>
	<link rel="stylesheet" href="~/css/footer/dark.css"/>@await RenderSectionAsync("OuterStyles", false)@await RenderSectionAsync("DeferScripts", false)
	<meta property="og:type" content="website"/><meta property="og:image" content="/favicon.svg"/>
	<meta property="og:site_name" content="execute233的网站"/><meta property="og:locale" content="zh_CN"/>
	<meta property="og:title" content="@ViewData["Title"]|execute233的网站" /><meta property="og:description" content="@ViewData["Description"]"/>
	<meta property="og:url" content="@(ViewData["OGUrl"] ?? $"{Context.Request.Scheme}://{Context.Request.Host}{Context.Request.PathBase}{Context.Request.Path}{Context.Request.QueryString}")"/>
	<meta name="keywords" content="@ViewData["Keywords"]"/><meta name="description" content="@ViewData["Description"]"/>
	<title>@ViewData["Title"]|execute233的网站</title>@await RenderSectionAsync("InnerStyles", false)@await RenderSectionAsync("PreloadScripts", false)@await RenderSectionAsync("HeadAddition", false)
</head>
<body>
	@{var navHTML = await RenderSectionAsync("nav-links", false);}
	<nav>
		<div id="nav-logo"><a id="nav-logo-link" target="_self" href="~/"><img id="nav-logo-image" src="~/favicon.svg" alt="Logo" /><span id="nav-logo-text">execute233的网站</span></a></div>
		<div id="nav-links">@if (navHTML is null) {<a target="_self" href="~/web/">目录</a><a target="_self" href="~/web/contact">联系站长</a><a target="_self" href="~/web/friendLinks">友情链接</a>} else @navHTML</div>
	</nav>
	<header>@await RenderSectionAsync("Header", false)</header>
	<main>@RenderBody()</main>
	<footer>
		<div id="mainCopyright">Copyright © 2022-@DateTime.Now.Year execute233. All rights reserved. &nbsp; execute233 版权所有</div>@await RenderSectionAsync("FooterAddition", false)
	</footer>@await RenderSectionAsync("EndScripts", false)@await RenderSectionAsync("BodyAddition", false)
</body>
</html>